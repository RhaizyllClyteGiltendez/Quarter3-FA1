var userConsent = confirm("Do you agree to share your personal information with our site?");

if (userConsent) {
  console.log("User agrees to share personal information.");
  console.log("Name: Clyte\nHeight: 5'0\"\nWeight: 99.208 lbs");
  alert("User agrees to share personal information.\nNickname: Clyte\nHeight: 5'0\"\nWeight: 99.208 lbs");
} else {
  console.log("User does not wish to share their information.");
  alert("User does not wish to share their information.");
}
